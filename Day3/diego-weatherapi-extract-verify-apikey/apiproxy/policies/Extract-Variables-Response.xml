<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-Variables-Response">
    <DisplayName>Extract Variables Response</DisplayName>
    <FaultRules/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Source clearPayload="false">response</Source>
    <VariablePrefix>title</VariablePrefix>
    <XMLPayload stopPayloadProcessing="false">
        <Variable name="name" type="string">
            <XPath>//title</XPath>
        </Variable>
    </XMLPayload>
</ExtractVariables>
